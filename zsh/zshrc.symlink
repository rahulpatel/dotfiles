export ZSH=~/.oh-my-zsh

ZSH_THEME="vercel"

# Just update
DISABLE_UPDATE_PROMPT="true"

# Update zsh every x days
UPDATE_ZSH_DAYS=7

# Don't check for untracked files in git repos
DISABLE_UNTRACKED_FILES_DIRTY="true"

plugins=(
  git
  zsh-autosuggestions
  zsh-syntax-highlighting
)

source $ZSH/oh-my-zsh.sh

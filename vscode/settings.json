{
    "editor.fontFamily": "Fira Code, RecursiveMonoLnr-Regular, RecursiveMonoCsl-Regular, CodeNewRoman Nerd Font Mono",
    "editor.tabSize": 2,
    "editor.cursorStyle": "line-thin",
    "editor.cursorBlinking": "smooth",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontLigatures": true,
    // "editor.fontLigatures": "'ss01','ss05','dlig'", // font ligatures for recursive mono
    "editor.formatOnSave": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.localityBonus": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.list.horizontalScrolling": true,
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.startupEditor": "welcomePage",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.restoreViewState": false,
    "window.restoreWindows": "all",
    "window.newWindowDimensions": "maximized",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "javascript.suggest.autoImports": false,
    "workbench.colorTheme": "Material Theme Palenight High Contrast",
    "prettier.requireConfig": true,
    "editor.suggestSelection": "first",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.panel.defaultLocation": "right",
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "search.followSymlinks": false,
    "search.exclude": {
        "*_api/lib": true,
        "*_ui/lib": true,
        "**/__pycache__": true,
        "**/.cache-loader": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.virtualenv": true,
        "**/.yarn-cache": true,
        "**/CVS": true,
        "**/lib/**/dist": true,
        "**/node_modules": true,
        "**/venv": true,
        "styleguide_docs/lib": true,
        "**/.next": true,
    },
    "files.exclude": {
        "*_api/lib": true,
        "*_ui/lib": true,
        "**/__pycache__": true,
        "**/.cache-loader": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.virtualenv": true,
        "**/.yarn-cache": true,
        "**/CVS": true,
        "**/lib/**/dist": true,
        "**/node_modules": true,
        "**/venv": true,
        "styleguide_docs/lib": true,
        "**/.next": true,
    },
    "files.watcherExclude": {
        "*_api/lib": true,
        "*_ui/lib": true,
        "**/__pycache__": true,
        "**/.cache-loader": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.virtualenv": true,
        "**/.yarn-cache": true,
        "**/CVS": true,
        "**/karma-*": true,
        "**/lib/**/dist": true,
        "**/node_modules": true,
        "**/node_modules/**": true,
        "**/venv": true,
        "styleguide_docs/lib": true,
        "**/.next": true,
    },
    "problems.showCurrentInStatus": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "window.zoomLevel": 1,
    "files.associations": {
        "Fastfile": "ruby",
        "Appfile": "ruby",
        "Gymfile": "ruby",
        "Matchfile": "ruby",
        "Pluginfile": "ruby"
    },
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.arrowParens": "always",
    "prettier.trailingComma": "es5",
    "workbench.fontAliasing": "auto",
    "eslint.alwaysShowStatus": true,
    "workbench.colorCustomizations": {},
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.languageServer": "Microsoft",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
    "workbench.activityBar.visible": false,
    "workbench.tree.indent": 21,
    "workbench.settings.useSplitJSON": true,
    "projectManager.git.baseFolders": [
        "/Volumes/Repositories/"
    ],
    "explorer.decorations.badges": false,
    "editor.renderLineHighlight": "all",
    "editor.fontSize": 13,
}
